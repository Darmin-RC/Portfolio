---
import Navbar from "../components/Navbar.astro";
import Skill from "../components/Skill.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="Darmin RC | Skills">
  <Navbar />
  <div class="skills">
    <div class="title">
      <h2>My <span>Skills</span></h2>
    </div>
    <Skill
      title="FrontEnd"
      img="https://upload.wikimedia.org/wikipedia/commons/b/bf/Front-end-logo-color%402x.png"
      description="FrontEnd developer with the latest technologies available on the market."
    />
    <Skill
      title="BackEnd"
      img="https://auth0.com/docs/img/quickstarts/backend-quantum.svg"
      description="Backend developer with the most popular and versatile languages ​​of the moment."
    />
    <Skill
      title="Design"
      img="https://seahawkmedia.com/wp-content/uploads/2020/12/8-logo-design-trend-01.png"
      description="Design of interfaces and brands for companies and businesses."
    />
    <Skill
      title="Mobile"
      img="data:image/jpeg;base64,"
      description="Mobile application development for Android and IOS."
    />
    <Skill
      title="Desktop"
      img="https://png.pngtree.com/png-vector/20191119/ourmid/pngtree-desktop-icon-vector-design-png-image_2000936.jpg"
      description="Development of Applications and Systems in Windows, MacOS and Linux."
    />
    <Skill
      title="Hacking"
      img="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS5cSHnOMXuQPiWSf47-x4khwuw4tGDSJHSuw&usqp=CAU"
      description="Password recovery with brute force tools."
    />
  </div>
</Layout>

<style>
  .skills {
    padding: 1em;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    gap: 1em;
  }

  .title {
    display: flex;
    justify-content: center;
    width: 100%;
  }

  .title h2 {
    text-shadow: 2px 4px 10px #000;
  }
</style>
